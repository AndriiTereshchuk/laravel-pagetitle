{
    "name": "rephlux/pagetitle",
    "description": "Manage page title in Laravel views",
    "keywords": [
        "laravel",
        "title",
        "blade"
    ],
    "homepage": "https://github.com/rephluX/laravel-pagetitle",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris van Daele",
            "email": "engine_no9@gmx.net"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0",
        "illuminate/support": "~5.0.0|~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Rephlux\\PageTitle\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Rephlux\\PageTitle\\Test\\": "test"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rephlux\\PageTitle\\PageTitleServiceProvider"
            ],
            "aliases": {
                "PageTitle": "Rephlux\\PageTitle\\Facades\\PageTitle"
            }
        }
    }
}
