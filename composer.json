{
    "name": "atereshchuk/pagetitle",
    "description": "Manage page title in Laravel views",
    "keywords": [
        "laravel",
        "title",
        "blade"
    ],
    "homepage": "https://github.com/AndriiTereshchuk/laravel-pagetitle",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrii Tereshchuk",
            "email": "andrii@tereshch.uk"
        }
    ],
    "require": {
        "php": "^7.2|^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "~6|~7|~8|~9|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "ATereshchuk\\PageTitle\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ATereshchuk\\PageTitle\\Test\\": "test"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ATereshchuk\\PageTitle\\PageTitleServiceProvider"
            ],
            "aliases": {
                "PageTitle": "ATereshchuk\\PageTitle\\Facades\\PageTitle"
            }
        }
    }
}
